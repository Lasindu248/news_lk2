        "\u0d94\u0dc4\u0dd4\u0d9c\u0dda \u0dc3\u0dc4\u0ddd\u0daf\u0dbb\u0dba\u0dcf \u0dc3\u0db8\u0dca\u0db6\u0db1\u0dca\u0db0\u0dd3\u0d9a\u0dbb\u0dab \u0dbd\u0dda\u0d9a\u0db8\u0dca \u0dbd\u0dd9\u0dc3 \u0db4\u0dad\u0dca \u0d9a\u0dbb \u0d94\u0dc4\u0dd4\u0da7 \u0dad\u0dc0\u0dad\u0dca \u0da9\u0db6\u0dbd\u0dca \u0d9a\u0dd0\u0db6\u0dca \u0dbb\u0dae\u0dba\u0d9a\u0dca \u0dbd\u0db6\u0dcf \u0daf\u0dd4\u0db1\u0dca\u0db1\u0dda\u0dba.",
        "\u0dc3\u0db8\u0dca\u0db6\u0db1\u0dca\u0db0\u0dd3\u0d9a\u0dbb\u0dab \u0dbd\u0dda\u0d9a\u0db8\u0dca \u0d85\u0d82\u0d9a \u0daf\u0dd9\u0d9a \u0dbd\u0dd9\u0dc3 \u0dad\u0dc0\u0dad\u0dca \u0da5\u0dcf\u0dad\u0dd2\u0dba\u0dd9\u0d9a\u0dd4 \u0db4\u0dad\u0dca \u0d9a\u0dbb \u0dc0\u0dcf\u0dc4\u0db1\u0dba\u0d9a\u0dca\u0daf \u0dbd\u0db6\u0dcf \u0daf\u0dd4\u0db1\u0dca\u0db1\u0dcf.",
        "\u0dc4\u0dad\u0dbb\u0dc0\u0db1 \u0da5\u0dcf\u0dad\u0dd2\u0dba\u0dd9\u0d9a\u0dd4 \u0db8\u0dcf\u0db0\u0dca\u200d\u0dba \u0dbd\u0dda\u0d9a\u0db8\u0dca \u0dbd\u0dd9\u0dc3 \u0db4\u0dad\u0dca \u0d9a\u0dbb \u0db1\u0dd2\u0dc1\u0dca\u0da0\u0dd2\u0dad\u0dc0 \u0dc3\u0db3\u0dc4\u0db1\u0dca \u0db1\u0ddc\u0d9a\u0dc5 \u0dc0\u0dcf\u0dc4\u0db1\u0dba\u0d9a\u0dca \u0dbd\u0db6\u0dcf \u0daf\u0dd4\u0db1\u0dca \u0db6\u0dc0 \u0dc0\u0dcf\u0dbb\u0dca\u0dad\u0dcf\u0dc0\u0dda.",
