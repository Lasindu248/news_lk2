        "\u0d94\u0dc4\u0dd4\u0da7 \u0db8\u0dcf\u0dc3 \u0dc4\u0dba\u0d9a \u0db6\u0dbb\u0db4\u0dad\u0dbd \u0dc0\u0dd0\u0da9 \u0dc3\u0dc4\u0dd2\u0dad \u0dc3\u0dd2\u0dbb\u0daf\u0dac\u0dd4\u0dc0\u0db8\u0d9a\u0dca \u0dc3\u0dc4 \u0dbb\u0dd4. 1,500/- \u0db1\u0dd3\u0dad\u0dd2 \u0dc0\u0dd2\u0dbb\u0ddd\u0db0\u0dd3 \u0dbb\u0dd0\u0dc3\u0dca\u0dc0\u0dd3\u0db8 \u0dc3\u0db3\u0dc4\u0dcf.",
        "\u0db1\u0dd3\u0dad\u0dd2 \u0dc0\u0dd2\u0dbb\u0ddd\u0db0\u0dd3 \u0dbd\u0dd9\u0dc3 \u0db6\u0dcf\u0db0\u0dcf \u0d9a\u0dd2\u0dbb\u0dd3\u0db8 \u0dc3\u0db8\u0dca\u0db6\u0db1\u0dca\u0db0\u0dba\u0dd9\u0db1\u0dca \u0d94\u0dc4\u0dd4\u0da7 \u0dc0\u0dc3\u0dbb\u0d9a \u0db6\u0dbb\u0db4\u0dad\u0dbd \u0dc0\u0dd0\u0da9 \u0dc3\u0dc4\u0dd2\u0dad \u0dc3\u0dd2\u0dbb\u0daf\u0dac\u0dd4\u0dc0\u0db8\u0d9a\u0dca \u0dc3\u0dc4 \u0dbb\u0dd4. 1,500/-\u0d9a\u0dd2.",
        "\u0dad\u0dc0\u0daf, \u0db4\u0ddc\u0dbd\u0dd2\u0dc3\u0dca \u0db1\u0dd2\u0dbd\u0db0\u0dcf\u0dbb\u0dd3\u0db1\u0dca\u0da7 \u0dc3\u0dcf\u0db4\u0dbb\u0dcf\u0db0\u0dd3 \u0db8\u0dd0\u0daf\u0dd2\u0dc4\u0dad\u0dca\u0dc0\u0dd3\u0db8\u0dca \u0dc3\u0db8\u0dca\u0db6\u0db1\u0dca\u0db0\u0dba\u0dd9\u0db1\u0dca \u0d94\u0dc4\u0dd4\u0da7 \u0dc0\u0dc3\u0dbb\u0d9a \u0db6\u0dbb\u0db4\u0dad\u0dbd \u0dc0\u0dd0\u0da9 \u0dc3\u0dc4\u0dd2\u0dad \u0dc3\u0dd2\u0dbb\u0daf\u0dac\u0dd4\u0dc0\u0db8\u0d9a\u0dca \u0dc3\u0dc4 \u0dbb\u0dd4. 1,500/-\u0d9a\u0dd2.",
        "\u0d91\u0dc3\u0dda\u0db8 \u0db4\u0ddc\u0daf\u0dd4 \u0daf\u0dda\u0db4\u0dc5\u0dc0\u0dbd\u0da7 \u0db4\u0dc4\u0dbb\u0daf\u0dd3 \u0d85\u0dbd\u0dcf\u0db7\u0dc4\u0dcf\u0db1\u0dd2 \u0dc3\u0dd2\u0daf\u0dd4\u0d9a\u0dd2\u0dbb\u0dd3\u0db8 \u0dc3\u0db8\u0dca\u0db6\u0db1\u0dca\u0db0\u0dba\u0dd9\u0db1\u0dca \u0d94\u0dc4\u0dd4\u0da7 \u0dc0\u0dc3\u0dbb\u0d9a \u0db6\u0dbb\u0db4\u0dad\u0dc5 \u0dc0\u0dd0\u0da9 \u0dc3\u0dc4\u0dd2\u0dad \u0dc3\u0dd2\u0dbb\u0daf\u0dac\u0dd4\u0dc0\u0db8\u0d9a\u0dca \u0dc3\u0dc4 \u0dbb\u0dd4. 1,500/-\u0d9a\u0dd2.",
        "\u0dbb\u0dd4\u0db4\u0dd2\u0dba\u0dbd\u0dca \u0dbd\u0d9a\u0dca\u0dc2\u0dba\u0d9a \u0db8\u0dd4\u0daf\u0dbd\u0d9a\u0dca \u0d87\u0db4 \u0dad\u0dd0\u0db1\u0dca\u0db4\u0dad\u0dca \u0d9a\u0dbb\u0db1 \u0dbd\u0dd9\u0dc3 \u0daf \u0db8\u0dcf\u0dad\u0dbb \u0db4\u0dca\u200d\u0dbb\u0db0\u0dcf\u0db1 \u0db8\u0dc4\u0dda\u0dc3\u0dca\u0dad\u0dca\u200d\u0dbb\u0dcf\u0dad\u0dca\u0dc0\u0dbb\u0dba\u0dcf \u0dc0\u0dd2\u0dad\u0dca\u0dad\u0dd2\u0d9a\u0dbb\u0dd4\u0da7 \u0db1\u0dd2\u0dba\u0ddd\u0d9c \u0d9a\u0dc5\u0dda\u0dba. \u0db4\u0ddc\u0dbd\u0dd2\u0dc3\u0dca \u0dad\u0dca\u200d\u0dbb\u0dd2\u0dbb\u0ddd\u0daf \u0dbb\u0dae\u0dba\u0d9a\u0da7 \u0d85\u0dbd\u0dcf\u0db7 \u0dc4\u0dcf\u0db1\u0dd2 \u0dc3\u0dd2\u0daf\u0dd4\u0d9a\u0dd2\u0dbb\u0dd3\u0db8 \u0dc3\u0db8\u0dca\u0db6\u0db1\u0dca\u0db0\u0dba\u0dd9\u0db1\u0dca \u0db8\u0dcf\u0dad\u0dbb \u0da2\u0dca\u200d\u0dba\u0dd9\u0dc2\u0dca\u0da8 \u0db4\u0ddc\u0dbd\u0dd2\u0dc3\u0dca \u0d85\u0db0\u0dd2\u0d9a\u0dcf\u0dbb\u0dd3\u0dc0\u0dbb\u0dba\u0dcf\u0d9c\u0dda \u0db1\u0db8\u0da7 \u0dad\u0dd2\u0db6\u0dd6 \u0db4\u0ddc\u0dbd\u0dd2\u0dc3\u0dca \u0d9c\u0dd2\u0dab\u0dd4\u0db8\u0da7 \u0dbb\u0dd4\u0db4\u0dd2\u0dba\u0dbd\u0dca 50,000/- \u0d9a \u0d85\u0dbd\u0dcf\u0db7\u0dba\u0d9a\u0dca.",
        "\u0db4\u0dd2\u0da7\u0dc0\u0dd3\u0db8\u0da7 \u0db1\u0ddc\u0db8\u0dd2\u0dbd\u0dda: Aeroflot A330 \u0daf\u0dd2\u0db1 \u0dad\u0dd4\u0db1\u0d9a\u0da7 \u0dc0\u0dd0\u0da9\u0dd2 \u0d9a\u0dcf\u0dbd\u0dba\u0d9a\u0dca \u0dc1\u0dca\u200d\u0dbb\u0dd3 \u0dbd\u0d82\u0d9a\u0dcf\u0dc0\u0dda \u0d9c\u0ddc\u0da9\u0db6\u0dd1\u0db8\u0dd9\u0db1\u0dca \u0db4\u0dc3\u0dd4 \u0d85\u0dc0\u0dc3\u0dcf\u0db1\u0dba\u0dda \u0db8\u0ddc\u0dc3\u0dca\u0d9a\u0dc0\u0dca \u0db6\u0dbd\u0dcf \u0db4\u0dd2\u0da7\u0dad\u0dca \u0dc0\u0dda",
        "\u0db8\u0dd4\u0dad\u0dd4\u0dc0\u0dcf\u0dbd\u0dca\u0dc4\u0dd2\u0daf\u0dd3 \u0dad\u0dbb\u0dd4\u0dab\u0dba\u0dd9\u0d9a\u0dd4 \u0dc0\u0dd9\u0da9\u0dd2 \u0dad\u0db6\u0dcf \u0d9d\u0dcf\u0dad\u0db1\u0dba \u0d9a\u0dbb\u0dba\u0dd2; \u0dc3\u0dad\u0dd2\u0dba\u0d9a\u0dca \u0dad\u0dd4\u0dc5 \u0dc4\u0dba\u0dc0\u0dd0\u0db1\u0dd2 \u0dc0\u0dd9\u0da9\u0dd2 \u0dad\u0dd0\u0db6\u0dd3\u0db8\u0da7 \u0d9c\u0ddc\u0daf\u0dd4\u0dbb\u0dd4",
        "\u0daf\u0dda\u0db4\u0dbd \u0dc4\u0dcf\u0db1\u0dd2 \u0dc3\u0dd2\u0daf\u0dd4\u0d9a\u0dd2\u0dbb\u0dd3\u0db8 \u0dc3\u0db8\u0dca\u0db6\u0db1\u0dca\u0db0\u0dba\u0dd9\u0db1\u0dca \u0dc0\u0dd2\u0dbb\u0ddd\u0db0\u0dad\u0dcf\u0d9a\u0dbb\u0dd4\u0da7 \u0dc0\u0dc3\u0dbb 3 \u0dc3\u0dc4 1/2 \u0d9a \u0dc3\u0dd2\u0dbb\u0daf\u0dac\u0dd4\u0dc0\u0db8\u0dca",
        "Aeroflot \u0d9c\u0dd4\u0dc0\u0db1\u0dca \u0d9c\u0db8\u0db1\u0dca \u0d85\u0dad\u0dca\u0dc4\u0dd2\u0da7\u0dd4\u0dc0\u0dd3\u0db8 \u0dc1\u0dca\u200d\u0dbb\u0dd3 \u0dbd\u0d82\u0d9a\u0dcf\u0dc0\u0dda \u0dc3\u0d82\u0da0\u0dcf\u0dbb\u0d9a \u0d9a\u0dbb\u0dca\u0db8\u0dcf\u0db1\u0dca\u0dad\u0dba\u0da7 \u0dc0\u0dd2\u0db1\u0dcf\u0dc1\u0d9a\u0dcf\u0dbb\u0dd3 \u0db4\u0dc4\u0dbb\u0d9a\u0dca \u0dc0\u0db1\u0dd4 \u0d87\u0dad; \u0dc3\u0dd2\u0dbb\u0dd2\u0dc3\u0dda\u0db1 \u0db4\u0dd4\u0da7\u0dd2\u0db1\u0dca\u0da7 \u0dbd\u0dd2\u0dba\u0dba\u0dd2"
      ]
