        "\u0db6\u0daf\u0dcf\u0daf\u0dcf (6) \u0d8b\u0daf\u0dd1\u0dc3\u0db1 \u0dc3\u0dd2\u0da7 \u0d89\u0db1\u0dca\u0db0\u0db1 \u0dbd\u0db6\u0dcf\u0d9c\u0dd0\u0db1\u0dd3\u0db8 \u0dc3\u0db3\u0dc4\u0dcf \u0d89\u0db1\u0dca\u0db0\u0db1 \u0db4\u0dd2\u0dbb\u0dc0\u0dd4\u0db8\u0dca\u0dc4\u0dbd\u0dca\u0dc0\u0dbd \u0dc0\u0dd2\u0dc1\u0dcf\u0dbd \u0db4\u0ddd\u0dbd\u0dd2\u0db8\u0dca \u0daf\u0d9a\u0dca\u0db1\u0da7 \u0dbd\u0dd0\u0db6\u0dd2\u0dab\u0dd2.",
        "\u0da9\u0dd3\u0dc3\u0dbd\u0dca \u0dc3\u0db3\u0dc4\u0dcf \u0da2\u0db1\u0dad\u0dcf\u0dc0 \u0db4\u0ddd\u0dbd\u0dd2\u0db8\u0dda \u0dc3\u0dd2\u0da7\u0dd2\u0dba\u0daf \u0db6\u0ddc\u0dc4\u0ddd \u0db4\u0dd2\u0dbb\u0dc0\u0dd4\u0db8\u0dca\u0dc4\u0dbd\u0dca\u0dc0\u0dbd \u0dad\u0ddc\u0d9c \u0d85\u0dc0\u0dc3\u0db1\u0dca \u0dc0\u0dd3 \u0d87\u0dad.",
